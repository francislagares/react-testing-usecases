    "@tanstack/query-core": "^5.53.1",
    "@tanstack/react-query": "^5.53.1",
    "@tanstack/react-query-devtools": "^5.53.1",
    "axios": "^1.7.5",
    "delay": "^6.0.0",
    "json-server": "1.0.0-beta.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0",
